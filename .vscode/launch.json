            "program": "${workspaceFolder}/bin/Debug/net8.0/win-x64/Printune.dll",
            "args" : [
                "PackagePrinter", "-PrinterName", "B38U", "-ExportPreferences", "-Output", "C:\\Temp\\B38U", "-IntuneWinUtil", "C:\\Tools\\IntuneWinAppUtil.exe"

    "version": "0.2.0",
    "configurations": [
        {
            "preLaunchTask": {
                "task": "build",
                "type": "dotnet"
            },
            "name": "Help",
            "type": "coreclr",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/bin/Debug/net8.0/win-x64/Printune.dll",
            "console": "internalConsole",
            "args": [ "Help", "-Template" ],
            "env": {
                "intunewinutil": "C:\\Tools\\IntuneWinAppUtil.exe",
            }
        },
        {
            "preLaunchTask": {
                "task": "build",
                "type": "dotnet"
            },
            "name": "InstallPrinter",
            "type": "coreclr",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/bin/Debug/net8.0/win-x64/Printune.dll",
            "args" : [
                "InstallPrinter", "-PrinterName", "B38U", "-Config", "C:\\temp\\B38U\\config.json"
            ],
            "console": "internalConsole",
            "env": {
                "intunewinutil": "C:\\Tools\\IntuneWinAppUtil.exe",
            }
        },
        {
            "preLaunchTask": {
                "task": "build",
                "type": "dotnet"
            },
            "name": "RemovePrinter",
